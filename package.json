{
  "name": "varl",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "m2c": "node package.json.type.to.commonjs.mjs",
    "c2m": "node package.json.type.to.module.js",
    "predev": "yarn m2c",
    "prenodev": "yarn c2m",
    "nodev": "node-dev nodev.mjs",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "dependencies": {
    "nuxt": "^2.15.3",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "colors": "^1.4.0",
    "date-utils": "^1.2.21",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-nuxt": "^1.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-local-rules": "file:node_modules_local/eslint-plugin-local-rules",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "eslint-plugin-vue-types": "^2.0.0",
    "fibers": "^5.0.0",
    "fs-extra": "^10.0.0",
    "node-dev": "^7.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.34.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "vue-types": "^2.0.3"
  }
}
